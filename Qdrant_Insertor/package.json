{
  "name": "qdrant-mcp-rag",
  "version": "1.0.0",
  "type": "module",
  "description": "Qdrant MCP RAG System",
  "private": true,
  "workspaces": [
    "packages/*",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:frontend": "npm run dev --workspace=packages/frontend",
    "build": "npm run build --workspaces",
    "build:backend": "npm run build --workspace=packages/backend",
    "build:frontend": "npm run build --workspace=packages/frontend",
    "build:shared": "npm run build --workspace=shared",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "format": "prettier --write \"packages/**/*.{ts,tsx,js,jsx,css,md}\" \"shared/**/*.{ts,js,md}\"",
    "type-check": "npm run type-check --workspaces",
    "clean": "npm run clean --workspaces && rm -rf node_modules",
    "install:all": "npm install && npm run build:shared",
    "test": "npm run test --workspaces",
    "check-lines" : "npm run format && node ./Scripts/check-lines.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}