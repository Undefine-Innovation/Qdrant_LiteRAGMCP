# 测试环境配置文件
NODE_ENV=test
JEST_WORKER_ID=1

# OpenAI配置 - 测试环境使用虚拟密钥
OPENAI_API_KEY=test-key-for-integration-tests
OPENAI_BASE_URL=https://api.openai.com

# 数据库配置 - 使用内存数据库
DB_PATH=:memory:

# Qdrant配置 - 测试环境
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=test-collection

# 向量配置
EMBEDDING_DIM=1536
EMBEDDING_BATCH_SIZE=10

# API配置
API_PORT=0

# 日志配置
LOG_LEVEL=error

# 测试特定配置
TEST_TIMEOUT=30000
TEST_DB_RESET=true