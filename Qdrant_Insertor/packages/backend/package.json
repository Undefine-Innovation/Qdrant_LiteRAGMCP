{
  "name": "qdrant-insertor-backend",
  "version": "1.0.0",
  "description": "Backend service for Qdrant MCP RAG system",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev:serve": "tsx watch src/index.ts",
    "build:compile": "npx tsc --build",
    "build:check": "npx tsc --noEmit",
    "test:unit": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.js",
  "test:integration": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.integration.config.js",
  "test:e2e-golden": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.integration.config.js --testPathPatterns=\"e2e|golden\"",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.js --coverage",
    "test:coverage:check": "node scripts/coverage-simple.js",
    "test:coverage:enhanced": "node scripts/coverage-enhanced.js",
    "test:coverage:unit": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.js --coverage --testPathPatterns='__tests__/unit'",
    "test:coverage:integration": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.integration.config.js --coverage --testPathPatterns='__tests__/integration'",
    "test:coverage:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.js --coverage --watch",
    "test:coverage:report": "node scripts/coverage-simple.js --check-only",
    "test:coverage:open": "node scripts/coverage-enhanced.js --open",
    "test:coverage:trend": "node scripts/coverage-trend-check.js",
    "test:quality-gate": "node scripts/coverage-enhanced.js --generate-report",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:check": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "npx eslint \"src/**/*.ts\" --fix",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "clean:build": "tsc --build --clean",
    "clean:coverage": "rm -rf coverage"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.0",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/winston": "^2.4.4",
    "better-sqlite3": "^12.2.0",
    "cheerio": "^1.1.2",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "formdata-node": "^6.0.3",
    "glob": "^11.0.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^5.16.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "swagger-docs": "^0.0.0-alpha2",
    "typeorm": "^0.3.17",
    "typescript-eslint": "^8.46.3",
    "undici": "^6.20.1",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.38.0",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.1.11",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "jest-json-reporter": "^1.2.2",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
