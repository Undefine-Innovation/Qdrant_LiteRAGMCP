{
  "name": "qdrant-insertor-backend",
  "version": "1.0.0",
  "description": "Backend service for Qdrant MCP RAG system",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "clean-build": "tsc --build --clean"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.0",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/winston": "^2.4.4",
    "better-sqlite3": "^12.2.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "formdata-node": "^6.0.3",
    "glob": "^11.0.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^5.16.0",
    "undici": "^6.20.1",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
